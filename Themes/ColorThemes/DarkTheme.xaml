<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background Colors -->
    <Color x:Key="DarkerContrastColor">#22202F</Color>
    <Color x:Key="MainColor">#353340</Color>
    <Color x:Key="MainWindowColor">#272537</Color>

    <!-- Text Colors -->
    <Color x:Key="PrimaryTextColor">#CFCFCF</Color>
    <Color x:Key="SecondaryTextColor">LightGray</Color>

    <!-- Window Action Button -->
    <Color x:Key="WaExitColor">#DC2626</Color>
    <Color x:Key="WaButtonsColor">#737373</Color>

    <!-- Checkbox Colors -->
    <Color x:Key="CheckboxBorder">#3A506B</Color>
    <Color x:Key="CheckboxChecked">#2B4A6A</Color>
    <Color x:Key="CheckboxHover">#325A84</Color>
    <Color x:Key="CheckboxMark">LightGray</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource MainColor}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource DarkerContrastColor}"/>
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryTextColor}"/>
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="{StaticResource SecondaryTextColor}"/>
    <SolidColorBrush x:Key="MainWindowBrush" Color="{StaticResource MainWindowColor}"/>

    <!-- Window Action Brushes -->
    <SolidColorBrush x:Key="WaExitBrush" Color="{StaticResource WaExitColor}"/>
    <SolidColorBrush x:Key="WaButtonsBrush" Color="{StaticResource WaButtonsColor}"/>
    
    <!-- Checkbox Brushes -->
    <SolidColorBrush x:Key="CheckboxBorderBrush" Color="{StaticResource CheckboxBorder}"/>
    <SolidColorBrush x:Key="CheckboxCheckedBrush" Color="{StaticResource CheckboxChecked}"/>
    <SolidColorBrush x:Key="CheckboxHoverBrush" Color="{StaticResource CheckboxHover}"/>
    <SolidColorBrush x:Key="CheckboxMarkBrush" Color="{StaticResource CheckboxMark}"/>
</ResourceDictionary>